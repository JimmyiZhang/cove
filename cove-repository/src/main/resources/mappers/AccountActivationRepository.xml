<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.carbybus.cove.repository.AccountActivationRepository">
    <select id="selectByCode" resultType="com.carbybus.cove.domain.entity.user.AccountActivation">
        SELECT id, user_id, user_code, create_time, expired_time FROM account_activation
        WHERE name = #{name}
    </select>
</mapper>