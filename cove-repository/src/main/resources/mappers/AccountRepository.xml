<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.carbybus.cove.repository.AccountRepository">
    <select id="selectByName" resultType="com.carbybus.cove.domain.entity.company.Account">
        SELECT id, user_name, password, salt, create_time, expired_time FROM `account`
        WHERE user_name = #{name}
    </select>
</mapper>